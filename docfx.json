{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**/*/*.csproj"
                    ],
                    "src": "Extractor/Out"
                }
            ],
            "dest": "api",
            "filter": "filterConfig.yml",
            "disableGitFeatures": true,
            "properties": {
                "TargetFramework": "netstandard2.1"
            },
            "namespaceLayout": "nested"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**/*.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    ".nojekyll",
                    "logo.png",
                    "banner.png"
                ]
            }
        ],
        "dest": "_site",
        "template": [
            "default",
            "modern",
            "templates/material"
        ],
        "postProcessors": [
            "ExtractSearchIndex"
        ],
        "globalMetadata": {
            "_appTitle": "DocumentationWarning",
            "_appLogoPath": "banner.png",
            "_appFaviconPath": "logo.png",
            "_enableSearch": true
        },
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ]
    }
}